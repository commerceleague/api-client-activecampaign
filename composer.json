        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "php-http/discovery": "^1.6",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
