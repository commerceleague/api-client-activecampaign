        "phpunit/phpunit": "~6.5.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CommerceLeague\\ActiveCampaignApi\\": "src/"
        }
